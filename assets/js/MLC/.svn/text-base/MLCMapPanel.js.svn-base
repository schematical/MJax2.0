 var MLCMapPanel = {
     Init: function() {
         this.arrMapKeys = new Array();
         this.arrMaps = new Array();
         this.arrPoints = new Array();
         this.LoadMap = function(strControlId, fltLat, fltLong, intZoom){
            
            var objLatlng = new google.maps.LatLng(fltLat, fltLong);
            var objOptions = {
              zoom: intZoom,
              center: objLatlng,
              mapTypeId: google.maps.MapTypeId.ROADMAP
            };
            
            var objMap = new google.maps.Map(document.getElementById(strControlId), objOptions);
            var intIndex = this.arrMapKeys.length;
            
            this.arrMapKeys[intIndex] = strControlId;
            this.arrMaps[intIndex] = objMap;
         };
         this.GetMap = function(strControlId){
             for(var i = 0; i < this.arrMapKeys.length; i++){
                 if(this.arrMapKeys[i] == strControlId){
                     return this.arrMaps[i];
                 }
                 return null;
             }
         };
         this.AddPoint = function(strControlId, fltLat, fltLong, strTitle){
            var objMap = this.GetMap(strControlId);
            var objLatlng = new google.maps.LatLng(fltLat, fltLong);
            this.objMarker = new google.maps.Marker({
              position: objLatlng,
              map: objMap,
              title:strTitle
            });
         };
         MLC.MarkLoaded('MLCMapPanel');
     }
     
 }
 MLCMapPanel.Init();